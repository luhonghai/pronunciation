<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  id="WebApp_ID" version="2.5">

    <servlet>
        <display-name>VoiceRecordHandler</display-name>
        <servlet-name>VoiceRecordHandler</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.VoiceRecordHandler</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>VoiceRecordHandler</servlet-name>
        <url-pattern>/VoiceRecordHandler</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>VoiceAnalyzeHandler</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.VoiceAnalyzeHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VoiceAnalyzeHandler</servlet-name>
        <url-pattern>/VoiceAnalyzeHandler</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>RegisterHandler</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.RegisterHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterHandler</servlet-name>
        <url-pattern>/RegisterHandler</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AuthHandler</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.AuthHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthHandler</servlet-name>
        <url-pattern>/AuthHandler</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FeedbackHandler</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.FeedbackHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FeedbackHandler</servlet-name>
        <url-pattern>/FeedbackHandler</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SummaryReport</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.SummaryReportHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SummaryReport</servlet-name>
        <url-pattern>/SummaryReport</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Feedbacks</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.Feedbacks</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Feedbacks</servlet-name>
        <url-pattern>/Feedbacks</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LicenseHandler</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.LicenseHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LicenseHandler</servlet-name>
        <url-pattern>/LicenseHandler</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LicenseCodes</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.LicenseCodes</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LicenseCodes</servlet-name>
        <url-pattern>/LicenseCodes</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ActivationHandler</servlet-name>
        <servlet-class>com.cmg.vrc.servlet.ActivationHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ActivationHandler</servlet-name>
        <url-pattern>/activate</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>dashboard.jsp</welcome-file>
    </welcome-file-list>
</web-app>
